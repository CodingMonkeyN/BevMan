{
  "name": "bev-man",
  "version": "0.0.1",
  "author": "Nils Braun",
  "scripts": {
    "ng": "ng",
    "start": "run-script-os",
    "generateApi": "rm -rf src/api && npx @openapitools/openapi-generator-cli generate -i ../wwwroot/swagger/v1/swagger.yaml -g typescript-angular -o src/api --additional-properties=fileNaming=kebab-case",
    "start:windows": "ng serve --port 4200 --ssl --ssl-cert \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem\" --ssl-key \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.key\"",
    "start:default": "ng serve --port 4200 --ssl --ssl-cert \"$HOME/.aspnet/https/${npm_package_name}.pem\" --ssl-key \"$HOME/.aspnet/https/${npm_package_name}.key\"",
    "ios": "npx cap run ios",
    "build:ios": "npm run build && npx cap sync ios && npx cap open ios",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
